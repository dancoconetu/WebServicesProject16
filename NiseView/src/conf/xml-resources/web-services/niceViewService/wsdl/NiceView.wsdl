<?xml version="1.0" encoding="UTF-8"?>
<definitions name="NiceView" targetNamespace="http://NiceView.WS"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://NiceView.WS" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
    <types>
        <xsd:schema targetNamespace="http://NiceView.WS" xmlns:tns="http://NiceView.WS">
            <xsd:complexType name="hotelType">
                <xsd:sequence maxOccurs="1" minOccurs="1">
                    <xsd:element name="hotelName" type="xsd:string"></xsd:element>
                     <xsd:element name="adress" type="xsd:string"></xsd:element>
                      <xsd:element name="bookingNR" type="xsd:string"></xsd:element>
                       <xsd:element name="totalPrice" type="xsd:double"></xsd:element>
                    <xsd:element name="creditCardNeeded" type="xsd:boolean"></xsd:element>
                    <xsd:element name="reservationService" type="xsd:string"></xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="getHotelInput">
                <xsd:sequence>
                    <xsd:element name="city" type="xsd:string"></xsd:element>
                    <xsd:element name="arrival" type="xsd:date"></xsd:element>
                    <xsd:element name="departure" type="xsd:date"></xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="getHotelInputs" type="tns:getHotelInput"></xsd:element>
            <xsd:complexType name="hotelListType">
                <xsd:sequence>
                    <xsd:element name="newElement" type="tns:hotelType" maxOccurs="unbounded" minOccurs="0"></xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="getHotelOutput" type="tns:hotelListType"></xsd:element>
        </xsd:schema>
    </types>
    <message name="NiceViewOperationRequest">
        <part name="getHotelInputs" element="tns:getHotelInputs"/>
    </message>
    <message name="NiceViewOperationResponse">
        <part name="getHotelOutput" element="tns:getHotelOutput"/>
    </message>
    <portType name="getHotelPort">
        <operation name="getHotelList">
            <input name="input1" message="tns:NiceViewOperationRequest"/>
            <output name="output1" message="tns:NiceViewOperationResponse"/>
        </operation>
    </portType>
    <binding name="getHotelPortBinding" type="tns:getHotelPort">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="getHotelList">
            <soap:operation/>
            <input name="input1">
                <soap:body use="literal"/>
            </input>
            <output name="output1">
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>
    <service name="NiceViewService">
        <port name="getHotelPortBindingPort" binding="tns:getHotelPortBinding">
            <soap:address location="http://localhost:${HttpDefaultPort}/service"/>
        </port>
    </service>
</definitions>
