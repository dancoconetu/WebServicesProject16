<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="travelAgencySoapBpel"
    targetNamespace="http://enterprise.netbeans.org/bpel/TravelAgencySoapBpel/travelAgencySoapBpel"
    xmlns:tns="http://enterprise.netbeans.org/bpel/TravelAgencySoapBpel/travelAgencySoapBpel"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
    <import namespace="http://LameDuck.ws" location="http://localhost:8080/LameDuckSoap/LameDuckService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://NiceView.WS" location="http://localhost:8080/NiceView/NiceViewService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/NiceViewServiceWrapper" location="NiceViewServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/TravelAgencySoapBpel/src/TravelAgency" location="TravelAgency.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="TravelAgency" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelAgencySoapBpel/src/TravelAgency" partnerLinkType="tns:TravelAgency" myRole="TravelAgencyPortTypeRole"/>
        <partnerLink name="HotelPartnerLink" xmlns:tns="http://enterprise.netbeans.org/bpel/NiceViewServiceWrapper" partnerLinkType="tns:NiceViewPortLinkType" partnerRole="NiceViewPortRole"/>
        <partnerLink name="FlightPartnerLink" xmlns:tns="http://LameDuck.ws" partnerLinkType="tns:LameDuck" partnerRole="LameDuckPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="GetHotelsListOut" xmlns:tns="http://NiceView.WS" messageType="tns:NiceViewOperationResponse4"/>
        <variable name="GetHotelsListIn" xmlns:tns="http://NiceView.WS" messageType="tns:NiceViewOperationRequest4"/>
        <variable name="GetHotelsOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelAgencySoapBpel/src/TravelAgency" messageType="tns:getHotelsResponse"/>
        <variable name="GetHotelsIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelAgencySoapBpel/src/TravelAgency" messageType="tns:getHotelsRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="TravelAgency" operation="getHotels" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelAgencySoapBpel/src/TravelAgency" portType="tns:TravelAgencyPortType" variable="GetHotelsIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="GetHotelsIn"/>
                <to variable="GetHotelsListIn" part="input1"/>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="HotelPartnerLink" operation="getHotelsList" xmlns:tns="http://NiceView.WS" portType="tns:NiceViewPort"/>
        <assign name="Assign2">
            <copy>
                <from variable="GetHotelsListOut" part="output1"/>
                <to variable="GetHotelsOut"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="TravelAgency" operation="getHotels" xmlns:tns="http://j2ee.netbeans.org/wsdl/TravelAgencySoapBpel/src/TravelAgency" portType="tns:TravelAgencyPortType" variable="GetHotelsOut"/>
    </sequence>
</process>
